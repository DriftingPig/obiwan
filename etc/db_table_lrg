CREATE TABLE obiwan_lrg (
    id integer,
    seed integer,
    ra double precision,
    "dec" double precision,
    lrg_g double precision,
    lrg_r double precision,
    lrg_z double precision,
    lrg_w1 double precision,
    lrg_redshift double precision,
    lrg_rhalf double precision,
    lrg_n double precision,
    lrg_pa double precision,
    lrg_ba double precision
);


