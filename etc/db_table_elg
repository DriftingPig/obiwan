CREATE TABLE obiwan_elg (
    id integer,
    seed integer,
    ra double precision,
    "dec" double precision,
    elg_g double precision,
    elg_r double precision,
    elg_z double precision,
    elg_redshift double precision,
    elg_rhalf double precision,
    elg_n double precision,
    elg_pa double precision,
    elg_ba double precision
);


